<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bc.pmpheep.back.dao.PmphGroupFileDao">

	<!-- 新增 PmphGroupFile -->
    <insert id="addPmphGroup"  parameterType="PmphGroupFile"  useGeneratedKeys="true" keyProperty="id"  >
     	insert  into  pmph_group_file (group_id,member_id,file_id,   file_name,download) 
                     	       values(#{groupId},#{memberId},#{fileId},#{fileName},#{download}) ;
    </insert>
    
   <!-- 根据id删除一个 PmphGroupFile -->
	<delete id="deletePmphGroupById" parameterType="PmphGroupFile"   >
		delete from  pmph_group_file where where id=#{id} ;
	</delete>
	
	<!-- 更新 PmphGroupFile -->
	<update id="updatePmphGroupById"   parameterType="PmphGroupFile"  >
		update pmph_group_file  
		  set    group_id=#{groupId},member_id=#{memberId},file_id=#{fileId}, file_name=#{fileName},download=#{download} 
        where id=#{id} ;
	</update>
	
	<!-- 根据id查询一个 PmphGroupFile -->
	<select  id="deletePmphGroupById" parameterType="PmphGroupFile"  resultType="PmphGroupFile" >
		select * from pmph_group_file where id=#{id} ;
	</select>
	
</mapper>