<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bc.pmpheep.back.dao.PmphMessageDao">

	<!-- 新增 PmphMessage -->
    <insert id="addPmphMessage"  parameterType="PmphMessage"  useGeneratedKeys="true" keyProperty="id"  >
    	insert  into  pmph_message (msg_code,msg_type)  values(#{msgCode},#{msgType})  ;
    </insert>
    
   <!-- 根据id删除一个 PmphMessage -->
	<delete id="deletePmphMessageById" parameterType="PmphMessage"   >
		delete from  pmph_message where where id=#{id} ;
	</delete>
	
	<!-- 更新 PmphMessage -->
	<update id="updatePmphMessageById"   parameterType="PmphMessage"  >
		update pmph_message  set msg_code=#{msgCode} , msg_type=#{msgType}  where id=#{id} ;
	</update>
	
	<!-- 根据id查询一个 PmphGroup -->
	<select  id="getPmphMessageById" parameterType="PmphMessage"  resultType="PmphMessage" >
		select * from pmph_message where id=#{id} ;
	</select>
	
</mapper>